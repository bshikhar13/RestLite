{
  "manifest_version": 2,

  "name": "Rest-Lite",
  "description": "This Extension Motivates You",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "myPage.html"
  },

  "chrome_url_overrides" : {
    "newtab": "myPage.html"
  },
  
  "icons": { "16": "icon.png",
           "48": "icon48.png",
          "128": "icon128.png" },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}